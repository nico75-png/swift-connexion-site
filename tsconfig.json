{
  "files": [],
  "references": [{ "path": "./tsconfig.app.json" }, { "path": "./tsconfig.node.json" }],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@testing-library/react": ["./src/test-utils/rtl"],
      "@testing-library/user-event": ["./src/test-utils/userEvent"],
      "@testing-library/jest-dom/vitest": ["./src/test-utils/jestDom"]
    },
    "noImplicitAny": false,
    "noUnusedParameters": false,
    "skipLibCheck": true,
    "allowJs": true,
    "noUnusedLocals": false,
    "strictNullChecks": false
  }
}
